<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/project_1"
	app:startDestination="@id/homeFragment2">

	<fragment
		android:id="@+id/homeFragment2"
		android:name="com.example.classes.project_1.HomeFragment"
		android:label="HomeFragment"
		tools:layout="@layout/project_1_home">
		<action
			android:id="@+id/action_homeFragment2_to_webFragment"
			app:destination="@id/webFragment" />
	</fragment>
	<fragment
		android:id="@+id/webFragment"
		android:name="com.example.classes.project_1.WebFragment"
		android:label="WebFragment"
		tools:layout="@layout/project_1_web" />
</navigation>